=TRIM(LEFT('Working CRQ''s'!S2, MIN(IFERROR(FIND(" enhancement", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(FIND(" sast", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(FIND(" fix", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(FIND(" opera", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)))-1))