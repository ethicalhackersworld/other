=UNIQUE(TRIM(LEFT('Working CRQ''s'!S2:S10, MIN(IFERROR(SEARCH(" enhancement", 'Working CRQ''s'!S2:S10), LEN('Working CRQ''s'!S10)), IFERROR(SEARCH(" sast", 'Working CRQ''s'!S2:S10), LEN('Working CRQ''s'!S2:S10)), IFERROR(SEARCH(" fix", 'Working CRQ''s'!S2:S10), LEN('Working CRQ''s'!S2:S10)), IFERROR(SEARCH(" opera", 'Working CRQ''s'!S2:S10), LEN('Working CRQ''s'!S2:S10)))-1)))