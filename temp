=TRIM(LEFT('Working CRQ''s'!S2, MIN(IFERROR(SEARCH(" enhancement", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(SEARCH(" sast", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(SEARCH(" fix", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)), IFERROR(SEARCH(" opera", 'Working CRQ''s'!S2), LEN('Working CRQ''s'!S2)))-1))